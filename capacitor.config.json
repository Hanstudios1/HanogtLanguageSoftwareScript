{
  "appId": "com.hanogt.codev",
  "appName": "Hanogt Codev",
  "webDir": "out",
  "server": {
    "url": "https://hanogtcodev.vercel.app",
    "cleartext": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#09090b",
      "showSpinner": true,
      "spinnerColor": "#3b82f6"
    },
    "StatusBar": {
      "style": "Dark",
      "backgroundColor": "#09090b"
    }
  },
  "ios": {
    "contentInset": "automatic",
    "allowsLinkPreview": true
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": false
  }
}
